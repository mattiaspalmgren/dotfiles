{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.sideBar.location": "right",
    "workbench.editor.enablePreview": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.centeredLayoutAutoResize": false,
    "workbench.activityBar.location": "hidden",
    "workbench.activityBar.orientation": "vertical",
    "workbench.statusBar.visible": false,
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.preferredHighContrastLightColorTheme": "Visual Studio Dark",
    "workbench.colorCustomizations": {
        "editor.background": "#252525",
        "sideBar.background": "#252525",
        "titleBar.activeBackground": "#252525",
        "editorOverviewRuler.border": "#0000",
        "minimap.errorHighlight": "#252525",
        "minimap.warningHighlight": "#252525",
        "gitDecoration.modifiedResourceForeground": "#6897BB",
        "scrollbar.shadow": "#00000000"

    },
    "editor.lineDecorationsWidth": 150,
    "editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
    "editor.showFoldingControls": "never",
    "editor.fontLigatures": true,
    "editor.fontSize": 11,
    "editor.tabSize": 2,
    "editor.lineHeight": 22,
    "editor.renderControlCharacters": false,
    "editor.matchBrackets": "never",
    "editor.occurrencesHighlight": "off",
    "editor.renderLineHighlight": "none",
    "editor.selectionHighlight": false,
    "editor.detectIndentation": false,
    "editor.parameterHints.cycle": true,
    "editor.parameterHints.enabled": false,
    "editor.smoothScrolling": false,
    "editor.lightbulb.enabled": "off",
    "editor.codeLens": true,
    "editor.foldingStrategy": "indentation",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.stablePeek": false,
    "editor.foldingHighlight": false,
    "editor.guides.indentation": false,
    "editor.guides.highlightActiveIndentation": false,
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",

    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "explorer.decorations.badges": false,
    "explorer.fileNesting.expand": false,
    "explorer.decorations.colors": true,
    
    "macros": {
      "selectAndCloseExplorer": [
        "list.select",
        "workbench.action.toggleSidebarVisibility"
      ]
    },

    "vim.foldfix": true,
    "vim.useSystemClipboard": true,
    "vim.sneak": true,
    "vim.leader": "space",
    "vim.useCtrlKeys": false,
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>","w"],
        "commands": ["workbench.action.files.saveAll"]
      },
      {
        "before": ["<leader>","q"],
        "commands": ["workbench.action.closeActiveEditor"]
      },
      {
        "before": ["<leader>","tab"],
        "commands": ["workbench.action.nextEditor"]
      },
      {
        "before": ["<leader>","f"],
        "commands": ["workbench.action.findInFiles"]
      },
      {
        "before": ["<leader>","a"],
        "commands": ["workbench.action.showCommands"]
      },
      {
        "before": ["g","t"],
        "commands": ["editor.action.goToDeclaration"]
      },
      {
        "before": ["g","h"],
        "commands": ["editor.action.showHover"]
      },
      {
        "before": ["<leader>","1"],
        "commands": ["editor.foldLevel1"]
      },
      {
        "before": ["<leader>","2"],
        "commands": ["editor.foldLevel2"]
      },
      {
        "before": ["<leader>","v"],
        "commands": ["workbench.files.action.showActiveFileInExplorer"]
      },
      {
        "before": ["<leader>","b"],
        "commands": ["workbench.action.navigateBack"]
      },
      {
        "before": ["g","r"],
        "commands": ["git.revertSelectedRanges"]
      },
      {
        "before": ["<leader>","t"],
        "commands": ["extension.jumpToTest"]
      },
      {
        "before": ["<leader>","r"],
        "commands": ["extension.runJest"]
      },
      {
        "before": ["<leader>","d"],
        "commands": ["extension.debugJest"]
      },
      {
        "before": ["<leader>", "m"],
        "commands": ["bookmarks.toggle"]
      },
      {
        "before": ["<leader>", "g"],
        "commands": ["bookmarks.list"]
      },
      {
        "before": ["<leader>", "n"],
        "commands": ["workbench.action.editor.nextChange"]
      },
      {
        "before": ["<leader>", "p"],
        "commands": ["workbench.action.editor.previousChange"]
      },
      {
        "before": ["z", "o"],
        "commands": ["editor.unfoldRecursively"]
      },
      {
        "before": ["<leader>", "e"],
        "commands": ["editor.action.marker.next"]
      },
      {
        "before": ["<C-u>"],
        "after": ["H", "z", "z"]
      },
      {
        "before": ["<C-d>"],
        "after": ["L", "z", "z"]
      }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
      {
        "before": ["<C-u>"],
        "after": ["H", "z", "z"]
      },
      {
        "before": ["<C-d>"],
        "after": ["L", "z", "z"]
      },
      {
        "before": ["g","r"],
        "commands": ["git.revertSelectedRanges"]
      },
      {
        "before": ["<leader>","a"],
        "commands": ["workbench.action.showCommands"]
      }
    ],
    "vim.handleKeys": {
      "<C-u>": true,
      "<C-d>": true,
      "<C-r>": true
    },
    "zenMode.hideLineNumbers": false,
    "zenMode.fullScreen": false,
    "zenMode.hideStatusBar": false,
    "breadcrumbs.enabled": false,
    "git.decorations.enabled": true,
    "gitlens.menus": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.repositories.enabled": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": false,
    "gitlens.views.search.enabled": true,
    "gitlens.views.compare.enabled": true,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.compare.location": "scm",
    "gitlens.views.search.location": "scm",
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "editor.stickyScroll.enabled": false,
    "scm.diffDecorations": "gutter",
    "diffEditor.experimental.showEmptyDecorations": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.decorations.badges": false,
    "search.decorations.badges": false,
    "search.decorations.colors": false,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "chat.commandCenter.enabled": false,
    "extensions.ignoreRecommendations": true,
    "supermaven.enable": {
      "*": true
    },
    "continue.showInlineTip": false
  }
