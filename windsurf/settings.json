{
	"workbench.startupEditor": "newUntitledFile",
	"workbench.editor.enablePreview": false,
	"workbench.tree.renderIndentGuides": "none",
	"workbench.activityBar.location": "hidden",
	"workbench.activityBar.orientation": "vertical",
	"workbench.statusBar.visible": true,
	"workbench.colorTheme": "JetBrains New Dark",
	"workbench.preferredHighContrastLightColorTheme": "Visual Studio Dark",
	"workbench.colorCustomizations": {
		"editor.background": "#252525",
		"sideBar.background": "#252525",
		"titleBar.activeBackground": "#252525",
		"statusBar.background": "#333333",
		"editorOverviewRuler.border": "#0000",
		"minimap.errorHighlight": "#252525",
		"minimap.warningHighlight": "#252525",
		"gitDecoration.modifiedResourceForeground": "#6897BB",
		"scrollbar.shadow": "#00000000",
		"editorGroupHeader.tabsBackground": "#252525",
		"editorGroupHeader.tabsBorder": "#333333",
		"tab.activeBorder": "#252525",
		"tab.activeBorderTop": "#252525",
		"tab.border": "#333333"
	},
	"editor.lineDecorationsWidth": 100,
	"editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
	"editor.showFoldingControls": "never",
	"editor.fontLigatures": false,
	"editor.fontSize": 11,
	"editor.tabSize": 2,
	"editor.lineHeight": 22,
	"editor.renderControlCharacters": false,
	"editor.matchBrackets": "never",
	"editor.occurrencesHighlight": "off",
	"editor.renderLineHighlight": "none",
	"editor.selectionHighlight": false,
	"editor.detectIndentation": false,
	"editor.parameterHints.cycle": true,
	"editor.parameterHints.enabled": false,
	"editor.smoothScrolling": false,
	"editor.lightbulb.enabled": "off",
	"editor.codeLens": true,
	"editor.foldingStrategy": "auto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",
	"editor.gotoLocation.multipleDeclarations": "goto",
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.stablePeek": false,
	"editor.foldingHighlight": false,
	"editor.guides.indentation": false,
	"editor.guides.highlightActiveIndentation": false,
	"editor.scrollbar.vertical": "hidden",
	"editor.scrollbar.horizontal": "hidden",

	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.openEditors.visible": 0,
	"explorer.decorations.badges": false,
	"explorer.fileNesting.expand": false,
	"explorer.decorations.colors": true,

	"macros": {
		"selectAndCloseExplorer": [
			"list.select",
			"workbench.action.toggleSidebarVisibility"
		]
	},

	"vim.foldfix": true,

	"vim.useSystemClipboard": true,
	"vim.sneak": true,
	"vim.leader": "space",
	"vim.useCtrlKeys": false,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["]", "d"],
			"commands": ["editor.action.marker.next"]
		},
		{
			"before": ["[", "d"],
			"commands": ["editor.action.marker.prev"]
		},
		{
			"before": ["<leader>", "m"],
			"after": ["<leader>", "<leader>", "s"]
		},
		{
			"before": ["<leader>", "e"],
			"commands": ["workbench.action.toggleSidebarVisibility"]
		},
		{
			"before": ["<leader>", "w"],
			"commands": ["workbench.action.files.saveAll"]
		},
		{
			"before": ["<leader>", "q"],
			"commands": ["workbench.action.closeActiveEditor"]
		},
		{
			"before": ["<leader>", "/"],
			"commands": ["workbench.action.findInFiles"]
		},
		{
			"before": ["g", "t"],
			"commands": ["editor.action.goToDeclaration"]
		},
		{
			"before": ["g", "h"],
			"commands": ["editor.action.showHover"]
		},
		{
			"before": ["<leader>", "-"],
			"commands": ["workbench.files.action.showActiveFileInExplorer"]
		},
		{
			"before": ["<leader>", "b"],
			"commands": ["workbench.action.navigateBack"]
		},
		{
			"before": ["<leader>", "n"],
			"commands": ["workbench.action.editor.nextChange"]
		},
		{
			"before": ["<leader>", "p"],
			"commands": ["workbench.action.editor.previousChange"]
		},
		{
			"before": ["<leader>", "a"],
			"commands": ["workbench.action.showCommands"]
		},
		{
			"before": ["g", "r"],
			"commands": ["git.revertSelectedRanges"]
		},
		{ "before": ["<S-j>"], "commands": [":bprevious"] },
		{ "before": ["<S-k>"], "commands": [":bnext"] },
		{ "before": ["leader", "v"], "commands": [":vsplit"] },
		{ "before": ["<leader>", "e"], "commands": ["workbench.view.explorer"] },
		{ "before": ["<leader>", "o"], "commands": ["workbench.action.showAllSymbols"] },
		{ "before": ["<leader>", "f"], "commands": ["workbench.action.quickOpen"] }
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": ["g", "c"],
			"commands": ["editor.action.commentLine"]
		},
		{
			"before": ["g", "r"],
			"commands": ["git.revertSelectedRanges"]
		},
		{
			"before": ["<leader>", "a"],
			"commands": ["workbench.action.showCommands"]
		}
	],
	"vim.handleKeys": {
		"<C-u>": true,
		"<C-d>": true,
		"<C-r>": true
	},
	"zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
	"git.decorations.enabled": true,
	"gitlens.menus": false,
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"gitlens.hovers.enabled": false,
	"gitlens.codeLens.enabled": false,
	"gitlens.statusBar.enabled": false,
	"gitlens.views.repositories.enabled": false,
	"gitlens.views.fileHistory.enabled": true,
	"gitlens.views.lineHistory.enabled": false,
	"gitlens.views.search.enabled": true,
	"gitlens.views.compare.enabled": true,
	"gitlens.mode.statusBar.enabled": false,
	"gitlens.views.repositories.location": "scm",
	"gitlens.views.fileHistory.location": "scm",
	"gitlens.views.lineHistory.location": "scm",
	"gitlens.views.compare.location": "scm",
	"gitlens.views.search.location": "scm",
	"window.zoomLevel": 0,
	"editor.minimap.enabled": false,
	"editor.stickyScroll.enabled": false,
	"scm.diffDecorations": "gutter",
	"diffEditor.experimental.showEmptyDecorations": false,
	"workbench.editor.decorations.colors": false,
	"workbench.editor.decorations.badges": false,
	"search.decorations.badges": false,
	"search.decorations.colors": false,
	"window.commandCenter": false,
	"chat.commandCenter.enabled": false,
	"extensions.ignoreRecommendations": true,
  "continue.showInlineTip": false,
	"zenMode.fullScreen": false,
	"windsurf.autocompleteSpeed": "default",
	"windsurf.chatFontSize": "default",
	"windsurf.rememberLastModelSelection": true,
	"windsurf.openRecentConversation": true,
	"windsurf.explainAndFixInCurrentConversation": true,
	"vim.easymotion": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit",
		"source.fixAll": "explicit",
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit",
		"source.fixAll.ruff": "explicit",
		"source.organizeImports.ruff": "explicit"
	},
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": true,
	"windsurf.showSelectionActions": false,
	"windsurf.inlineSuggestions.enabled": false,
	"windsurf.showInlayHintShortcuts": false,
	"windsurf.showExplainProblemInlayHint": true,
	"python.analysis.indexing": true,
	"basedpyright.analysis.typeCheckingMode": "basic",
	"basedpyright.analysis.inlayHints.functionReturnTypes": false,
	"basedpyright.analysis.inlayHints.variableTypes": false,
	"basedpyright.analysis.inlayHints.callArgumentNames": false,
	"explorer.confirmPasteNative": false,
  "windsurf.autoExecutionPolicy": "off",
  "breadcrumbs.enabled": false
}
